{
  "name": "markfknight.dev",
  "private": true,
  "description": "The personal site for Mark Knight (markfknight)",
  "version": "3.0.0",
  "license": "MIT",
  "homepage": "https://markfknight.dev",
  "keywords": [
    "gatsby"
  ],
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "deploy": "gatsby build --prefix-paths && gh-pages --dotfiles -d public",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "type-check": "tsc --noEmit",
    "lint": "eslint -c .eslintrc.js --ext .ts,.tsx,.js src",
    "lint:fix": "npm run lint -- --fix",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "jest",
    "posttest": "npm run lint",
    "test:watch": "jest --watch",
    "test:watchAll": "jest --watchAll"
  },
  "dependencies": {
    "gatsby": "^4.2.0",
    "gatsby-plugin-catch-links": "^4.2.0",
    "gatsby-plugin-manifest": "^4.2.0",
    "gatsby-plugin-meta-redirect": "^1.1.1",
    "gatsby-plugin-offline": "^5.2.0",
    "gatsby-plugin-react-helmet": "^5.2.0",
    "gatsby-plugin-sharp": "^4.2.0",
    "gatsby-plugin-sitemap": "^5.2.0",
    "gatsby-plugin-verify-brave": "^1.0.5",
    "gatsby-redirect-from": "^0.3.0",
    "gatsby-remark-external-links": "0.0.4",
    "gatsby-remark-images": "^6.2.0",
    "gatsby-remark-katex": "^6.2.0",
    "gatsby-remark-responsive-iframe": "^5.14.0",
    "gatsby-remark-smartypants": "^5.2.0",
    "gatsby-remark-vscode": "^3.2.1",
    "gatsby-source-filesystem": "^4.2.0",
    "gatsby-transformer-remark": "^5.2.0",
    "katex": "^0.13.3",
    "monokai-operator-theme": "github:markfknight/monokai-operator",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.4.0",
    "@testing-library/jest-dom": "^5.11.10",
    "@testing-library/react": "^12.1.2",
    "@types/jest": "^27.0.3",
    "@types/node": "^16.11.10",
    "@types/react": "^17.0.11",
    "@types/react-dom": "^17.0.7",
    "@types/react-helmet": "^6.1.1",
    "@types/react-test-renderer": "^17.0.1",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "autoprefixer": "^10.2.5",
    "babel-jest": "^27.3.1",
    "babel-preset-gatsby": "^2.2.0",
    "eslint": "^8.3.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-jest": "^25.3.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-testing-library": "^5.0.0",
    "gatsby-plugin-postcss": "^5.2.0",
    "gh-pages": "^3.1.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.3.1",
    "postcss": "^8.2.15",
    "prettier": "2.4.1",
    "tailwindcss": "^2.1.2",
    "ts-jest": "^27.0.7",
    "typescript": "^4.2.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/markfknight/markfknight.github.io"
  },
  "bugs": {
    "url": "https://github.com/markfknight/markfknight.github.io/issues"
  }
}
