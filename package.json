{
  "name": "markfknight.dev",
  "private": true,
  "description": "The personal site for Mark Knight (markfknight)",
  "version": "2.0.0",
  "license": "MIT",
  "homepage": "https://markfknight.dev",
  "keywords": [
    "gatsby"
  ],
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "deploy": "gatsby build --prefix-paths && gh-pages --dotfiles -d public",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "type-check": "tsc --noEmit",
    "lint": "eslint -c .eslintrc.js --ext .ts,.tsx,.js src",
    "lint:fix": "npm run lint -- --fix",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:watchAll": "jest --watchAll"
  },
  "dependencies": {
    "gatsby": "^3.0.0",
    "gatsby-plugin-catch-links": "^3.0.0",
    "gatsby-plugin-manifest": "^3.0.0",
    "gatsby-plugin-meta-redirect": "^1.1.1",
    "gatsby-plugin-offline": "^4.0.0",
    "gatsby-plugin-react-helmet": "^4.0.0",
    "gatsby-plugin-sharp": "^3.0.0",
    "gatsby-plugin-sitemap": "^3.0.0",
    "gatsby-plugin-verify-brave": "^1.0.5",
    "gatsby-redirect-from": "^0.3.0",
    "gatsby-remark-external-links": "0.0.4",
    "gatsby-remark-images": "^4.0.0",
    "gatsby-remark-katex": "^4.0.0",
    "gatsby-remark-responsive-iframe": "^3.0.0",
    "gatsby-remark-smartypants": "^3.0.0",
    "gatsby-remark-vscode": "^3.2.0",
    "gatsby-source-filesystem": "^3.0.0",
    "gatsby-transformer-remark": "^3.0.0",
    "katex": "^0.12.0",
    "monokai-operator-theme": "github:markfknight/monokai-operator",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.4.0",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.5",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.31",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.1",
    "@types/react-helmet": "^6.1.0",
    "@types/react-test-renderer": "^17.0.1",
    "@typescript-eslint/eslint-plugin": "^4.16.1",
    "@typescript-eslint/parser": "^4.16.1",
    "autoprefixer": "^10.2.4",
    "babel-jest": "^26.6.3",
    "babel-preset-gatsby": "^1.0.0",
    "eslint": "^7.20.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-jest": "^24.1.5",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-testing-library": "^3.10.1",
    "gatsby-plugin-postcss": "^4.0.0",
    "gh-pages": "^3.1.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "postcss": "^8.2.6",
    "prettier": "2.2.1",
    "tailwindcss": "^2.0.3",
    "ts-jest": "^26.5.1",
    "typescript": "^4.1.5"
  },
  "repository": {
    "type": "git",
